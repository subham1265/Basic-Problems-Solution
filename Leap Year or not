import java.util.*;
/**
 *
 * @author subha
 */
public class JavaApplication2 {

  public static void main(String [] args){
    Scanner sc= new Scanner (System.in);
    System.out.print("Year : ");
    int Year= sc.nextInt();
    
    if(Year%4 ==0){
        System.out.println("This is a leap Year");
    }
    else{
        System.out.println("This is not a leap Year");
    }
   
} 

}
